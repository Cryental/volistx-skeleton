{
    "name": "volistx/framework",
    "description": "Volistx Framework For RESTful API Based on Laravel/Lumen",
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^8.1",
        "chuckrincon/lumen-config-discover": "^1.0",
        "cryental/laravel-stackpath": "^1.0",
        "fakerphp/faker": "^1.19",
        "illuminate/redis": "^9.1",
        "laravel/lumen-framework": "^9.0",
        "mlntn/lumen-artisan-serve": "^1.5",
        "monicahq/laravel-cloudflare": "^3.2",
        "spatie/laravel-responsecache": "^7.2",
        "volistx/framework-kernel": "dev-master",
        "volistx/lumen-rate-limiting": "^1.1",
        "wikimedia/ip-set": "^3.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.5",
        "phpunit/phpunit": "^9.5",
        "spatie/ray": "^1.33"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "platform": {
            "php": "8.1"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "start": "php -S localhost:7777 -t public"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/VolistxTeam/Framework-Kernel"
        }
    ]
}
