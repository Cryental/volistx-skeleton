{
    "name": "volistx/framework",
    "description": "Volistx Framework For RESTful API Based on Laravel/Lumen",
    "license": "MIT",
    "type": "project",
    "authors": [
        {
            "name": "Cryental",
            "email": "contact@cryental.dev"
        },
        {
            "name": "Mostafa Samy",
            "email": "mostafa.samy203@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.1",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-pdo": "*",
        "ext-zip": "*",
        "cryental/laravel-stackpath": "^1.1",
        "guzzlehttp/guzzle": "^7.2",
        "laravel/framework": "^9.45",
        "laravel/octane": "^1.3",
        "matriphe/larinfo": "^4.0",
        "spatie/laravel-ignition": "^1.6",
        "volistx/framework-kernel": "^6.2"
    },
    "require-dev": {
        "fakerphp/faker": "^1.21",
        "mockery/mockery": "^1.5",
        "nunomaduro/collision": "^6.3",
        "phpunit/phpunit": "^9.5.10",
        "spatie/ray": "^1.36"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        },
        "platform": {
            "php": "8.1",
            "ext-pcntl": "8.1",
            "ext-posix": "8.1"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
